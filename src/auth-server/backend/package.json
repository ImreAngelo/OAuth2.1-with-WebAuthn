  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "vitest --run",
    "test:coverage": "vitest run --coverage --run",
    "start": "pnpm dev",
    "build": "node esbuild.config.js",
    "dev": "tsx watch src/index.ts",
    "analyze": "pnpm build --analyze && pnpm analyze:convert && pnpm analyze:display",
