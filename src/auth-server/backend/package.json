    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "3.2.4",
    "esbuild": "^0.25.10",
    "http-proxy-middleware": "^3.0.5",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "ws": "^8.18.3"
